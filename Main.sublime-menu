									"contents": "{\n\t// Template if you want to make a color scheme\n\t\"globals\":\n\t// Suggest not to change\n\t{\n\t\t\"background\": \"#000000\",\n\t\t\"foreground\": \"#FFFFFF\",\n\t\t\"line_highlight\": \"#333333\",\n\t\t\"caret\": \"#FFFFFF\",\n\t\t\"block_caret\": \"#FFFFFF\",\n\t\t\"selection\": \"#363636\",\n\t\t\"selection_border\": \"#222222\",\n\t\t\"invisibles\": \"#222222\",\n\t\t\"misspelling\": \"#FF0000\",\n\t},\n\t\"rules\": \n\t[\n\t\t{\n\t\t\t// Default color (Don't change)\n\t\t\t\"scope\": \"source.mkb\",\n\t\t\t\"foreground\": \"#FFFFFF\"\n\t\t},\n\t\t{\n\t\t\t// Comments\n\t\t\t\"scope\": \"comment.mkb\",\n\t\t\t\"foreground\": \"${1:#AAAAAA}\",\n\t\t\t\"font_style\": \"italic\"\n\t\t},\n\t\t{\n\t\t\t// Parameters\n\t\t\t\"scope\": \"variable.parameter.mkb\",\n\t\t\t\"foreground\": \"${2:#FFAA00}\"\n\t\t},\n\t\t{\n\t\t\t// Strings\n\t\t\t\"scope\": \"string.mkb\",\n\t\t\t\"foreground\": \"${3:#55FF55}\"\n\t\t},\n\t\t{\n\t\t\t // Symbols eg %% , ; ( ) [ ]\n\t\t\t\"scope\": \"keyword.other.mkb\",\n\t\t\t\"foreground\": \"${4:#FF55FF}\"\n\t\t},\n\t\t{\n\t\t\t// Numbers\n\t\t\t\"scope\": \"constant.numeric.mkb\",\n\t\t\t\"foreground\": \"${5:#5555FF}\"\n\t\t},\n\t\t{\n\t\t\t// Actions\n\t\t\t\"scope\": \"keyword.control.mkb\",\n\t\t\t\"foreground\": \"${6:#AA00AA}\"\n\t\t},\n\t\t{\n\t\t\t// Built in variables\n\t\t\t\"scope\": \"variable.mkb\",\n\t\t\t\"foreground\": \"${7:#55FFFF}\"\n\t\t},\n\t\t{\n\t\t\t// Iterators\n\t\t\t\"scope\": \"keyword.operator.word.mkb\",\n\t\t\t\"foreground\": \"${8:#00AAAA}\"\n\t\t},\n\t\t{\n\t\t\t// User defined variables\n\t\t\t\"scope\": \"punctuation.definition.keyword.mkb\",\n\t\t\t\"foreground\": \"${9:#FF5555}\"\n\t\t},\n\t\t{\n\t\t\t// Operators eg = < > && ||\n\t\t\t\"scope\": \"keyword.operator.mkb\",\n\t\t\t\"foreground\": \"${10:#FFFF55}\"\n\t\t},\n\t\t{\n\t\t\t// False\n\t\t\t\"scope\": \"constant.language.false.mkb\",\n\t\t\t\"foreground\": \"${11:#AA0000}\"\n\t\t},\n\t\t{\n\t\t\t// True\n\t\t\t\"scope\": \"constant.language.true.mkb\",\n\t\t\t\"foreground\": \"${12:#00AA00}\"\n\t\t},\n\t\t{\n\t\t\t// Keybinds\n\t\t\t\"scope\": \"keyword.control.import.mkb\",\n\t\t\t\"foreground\": \"${13:#0000AA}\"\n\t\t},\n\t\t{\n\t\t\t// Invalid stuff\n\t\t\t\"scope\": \"invalid.mkb\",\n\t\t\t\"foreground\": \"${14:#FFFFFF}\",\n\t\t\t\"background\": \"${15:#AA0000}\"\n\t\t},\n\t\t{\n\t\t\t// Functions\n\t\t\t\"scope\": \"meta.function.mkb\",\n\t\t\t\"foreground\": \"${16:#B9F2FF}\"\n\t\t},\n\t\t// DO NOT CHANGE MINECRAFT COLOR CODES BELOW\n\t\t{\n\t\t\t\"scope\": \"0.mkb\",\n\t\t\t\"foreground\": \"black\",\n\t\t\t\"background\": \"white\"\n\t\t},\n\t\t{\n\t\t\t\"scope\": \"1.mkb\",\n\t\t\t\"foreground\": \"black\",\n\t\t\t\"background\": \"#0000AA\"\n\t\t},\n\t\t{\n\t\t\t\"scope\": \"2.mkb\",\n\t\t\t\"foreground\": \"black\",\n\t\t\t\"background\": \"#00AA00\"\n\t\t},\n\t\t{\n\t\t\t\"scope\": \"3.mkb\",\n\t\t\t\"foreground\": \"black\",\n\t\t\t\"background\": \"#00AAAA\"\n\t\t},\n\t\t{\n\t\t\t\"scope\": \"4.mkb\",\n\t\t\t\"foreground\": \"black\",\n\t\t\t\"background\": \"#AA0000\"\n\t\t},\n\t\t{\n\t\t\t\"scope\": \"5.mkb\",\n\t\t\t\"foreground\": \"black\",\n\t\t\t\"background\": \"#AA00AA\"\n\t\t},\n\t\t{\n\t\t\t\"scope\": \"6.mkb\",\n\t\t\t\"foreground\": \"black\",\n\t\t\t\"background\": \"#FFAA00\"\n\t\t},\n\t\t{\n\t\t\t\"scope\": \"7.mkb\",\n\t\t\t\"foreground\": \"black\",\n\t\t\t\"background\": \"#AAAAAA\"\n\t\t},\n\t\t{\n\t\t\t\"scope\": \"8.mkb\",\n\t\t\t\"foreground\": \"black\",\n\t\t\t\"background\": \"#555555\"\n\t\t},\n\t\t{\n\t\t\t\"scope\": \"9.mkb\",\n\t\t\t\"foreground\": \"black\",\n\t\t\t\"background\": \"#5555FF\"\n\t\t},\n\t\t{\n\t\t\t\"scope\": \"a.mkb\",\n\t\t\t\"foreground\": \"black\",\n\t\t\t\"background\": \"#55FF55\"\n\t\t},\n\t\t{\n\t\t\t\"scope\": \"b.mkb\",\n\t\t\t\"foreground\": \"black\",\n\t\t\t\"background\": \"#55FFFF\"\n\t\t},\n\t\t{\n\t\t\t\"scope\": \"c.mkb\",\n\t\t\t\"foreground\": \"black\",\n\t\t\t\"background\": \"#FF5555\"\n\t\t},\n\t\t{\n\t\t\t\"scope\": \"d.mkb\",\n\t\t\t\"foreground\": \"black\",\n\t\t\t\"background\": \"#FF55FF\"\n\t\t},\n\t\t{\n\t\t\t\"scope\": \"e.mkb\",\n\t\t\t\"foreground\": \"black\",\n\t\t\t\"background\": \"#FFFF55\"\n\t\t},\n\t\t{\n\t\t\t\"scope\": \"f.mkb\",\n\t\t\t\"foreground\": \"black\",\n\t\t\t\"background\": \"#FFFFFF\"\n\t\t}\n\t]\n}"
								}
